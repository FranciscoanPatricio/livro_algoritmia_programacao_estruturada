//Elabore um programa que imprime um factoria dum número

#include <stdio.h>

int main(){
    int numero, fatorial=0;

    printf("Insira um número:\n");
    scanf("%d", &numero);

    while(numero){
        if(fatorial==0){
            fatorial=numero;  
        }
        else if(numero!=1){
            fatorial=fatorial*numero;
        }
        if(numero==1)
            break;
        numero--;
        
    }
    printf("%d", fatorial);
    return 0;
}
