//introduzir dois numeros, comparar e imprimir o maior

#include <stdio.h>

int main (){
    float numero=0, numero1=0;

    printf("insira um número para comparar: \n");
    scanf("%f", &numero);
    printf("insira um número para comparar: \n");
    scanf("%f", &numero1);
    
    if(numero > numero1){
        printf("O numero maior é o %.2f.", numero);
    }else{
        printf("O numero maior é o %.2f.", numero1);
    }
    return 0;
}
