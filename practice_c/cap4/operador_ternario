//operador ternario
//Elaborar um programa que diz "parab√©ns" caso a nota seja maior que 10 e se for menor marcar um novo exame

#include <stdio.h>

int main(){
    float nota;
    printf("Insira a nota do aluno:\n");
    scanf("%f", &nota);

    if(nota>=10.0){
        printf("Foste aprovado!!!");
    }else{
        printf("Foste reprovado, remarca o exame");
    }

}
