//Recebe dois numeros, imprime qual o maior ou igual.

#include <stdio.h>

void texto(){
    printf("Insira um número: \n");
}

int main(){
    float number1, number2;

    texto();
    scanf("%f", &number1);
    texto();
    scanf("%f", &number2);

    if(number1>number2){
        printf("O número %.2f é maior que %.2f.\n", number1, number2);
}
    else if (number1<number2){
         printf("O número %.2f é maior que %.2f.\n", number2, number1);
}
else{
 printf("O número %.2f é igual a %.2f.\n", number1, number2);}
return 0;
}
