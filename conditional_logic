//AND/OR/BIGGER/SMALLER/EQQUAL/NEGATION
#include <stdio.h>

int main(){
    // 0 is false and  1 is true 
    printf("4=5 %d\n", 4==5); // equal
    printf("4|=6 %d\n", 4!=6);  //not equal
    printf("4>5 %d\n", 4 > 5); //bigger
    printf("4<5 e 6 >10 %d\n", 4>5 && 6 > 10); // both number be bigger
    printf("40 < 50 ou 60 > 90 %d\n", 40< 50 || 60 > 90); // one number be smaller or the other bigger
    printf("NÃ£o 40>5 ou 60 < 145 %d", !(40>5 || 60 < 145)); //negation of one number be smaller or the other bigger

    return 0;
}
